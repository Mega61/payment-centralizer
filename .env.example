# Server Configuration
PORT=8080
NODE_ENV=development
LOG_LEVEL=info

# GCP Configuration
GCP_PROJECT_ID=your-project-id
GCS_BUCKET_NAME=payment-centralizer-uploads
GCS_ANNOTATIONS_BUCKET_NAME=payment-centralizer-annotations

# Vision API Configuration
VISION_API_FEATURES=TEXT_DETECTION,DOCUMENT_TEXT_DETECTION,LOGO_DETECTION,LABEL_DETECTION

# OpenTelemetry Configuration
ENABLE_TRACING=true
ENABLE_METRICS=true
OTLP_ENDPOINT=http://localhost:4317

# Security Configuration
CORS_ORIGIN=*
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
REQUIRE_AUTHENTICATION=false
